<br />
<%= simple_form_for(@user_room_relation) do |f| %>
  <%= f.error_notification %>
  <div class="row">
    <div class="form-inputs">
      <%= f.input :room_id, label_html: { class: 'active' } %>
      <%= f.hidden_field :user_id %>
    </div>
  </div>
  <div class="row">
    <div class="form-actions col s8">
      <%= f.button :submit %>
    </div>
  </div>
<% end %>